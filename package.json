{
  "name": "3d-geometric-search",
  "version": "2.0.0",
  "description": "Professional 3D geometric model viewer with multi-format support (GLTF, OBJ, STL, FBX, STEP), section management, advanced navigation, keyboard shortcuts, and model export capabilities",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.{js,css,html}\""
  },
  "keywords": [
    "3d",
    "viewer",
    "geometry",
    "sections",
    "threejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "eslint": "^8.55.0",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.0.1",
    "prettier": "^3.1.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.4"
  }
}
