{
  "name": "3d-geometric-search",
  "version": "2.0.0",
  "description": "Professional 3D geometric search and visualization application with industry-standard format support",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,html}\"",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "3d",
    "geometry",
    "visualization",
    "three.js",
    "gltf",
    "step",
    "cad",
    "viewer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "three": "^0.160.0",
    "three-stdlib": "^2.29.0"
  },
  "devDependencies": {
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  }
}
